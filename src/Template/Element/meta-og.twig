<!-- OG dataset -->
<meta property="og:title" content="{{ object.title|default(publication.title)|raw|striptags}}" />
<meta property="og:type" content="{{ object.type}}" />
<meta property="og:url" content="" />
<meta property="og:description" content="{{ object.description|default(publication.description)|raw|striptags}}" />
<meta property="og:site_name" content="{{ publisher.title }}" />
<meta property="og:updated_time" content="{{ object.modified}}" />
<meta property="og:image" content="{{ Poster.url(object, 'default') }}" />
