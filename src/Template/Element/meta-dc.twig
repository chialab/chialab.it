<!-- RTF dublin core -->
<link rel="schema.DC" href="http://purl.org/dc/elements/1.1/" />
<meta name="DC.title" 		content="{{ object.title|default(publication.title)|striptags|raw|escape('html_attr') }}" />
<meta name="DC.description" content="{{ object.description|default(publication.description)|striptags|raw|escape('html_attr') }}" />
<meta name="DC.creator" 	content="{{ (object.has_author|first).title|default(publisher.title)|default('')|striptags|raw|escape('html_attr') }}" />
<meta name="DC.publisher" 	content="{{ (object.has_publisher|first).title|default(publisher.title)|default('')|striptags|raw|escape('html_attr') }}" />
<meta name="DC.date" 		content="{{ object.publish_start|default(object.created)|escape('html_attr') }}" />
<meta name="DC.created" 	content="{{ object.created|escape('html_attr') }}" />
<meta name="DC.modified" 	content="{{ object.modified|escape('html_attr') }}" />
<meta name="DC.type" 		content="{{ object.type|escape('html_attr') }}" />
<meta name="DC.format" 		content="text/html" />
<meta name="DC.identifier"  content="{{ object.uname|escape('html_attr') }}" />
<meta name="DC.language" 	content="{{ object.lang|default('it')|escape('html_attr') }}" />
<meta name="DC.rights" 		content="{{ (object.has_publisher|first).title|default(publisher.title)|default('')|striptags|raw|escape('html_attr') }}" />
<meta name="DC.license" 	content="{{ __('Creative Commons Attribution - share alike (CC BY-SA 4.0)')|raw|escape('html_attr') }}" />
