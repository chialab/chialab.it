{% if illustrators|length > 0 %}
    <dna-masonry class="w-full grid m-0">
        {% for illustrator in illustrators %}
            {{ cell('Chialab.Card::collapsable', { object: illustrator }, {
                cache: {
                    key: 'card_collapsable_' ~ locale ~ '_' ~ illustrator.id ~ '_' ~ illustrator.modified.toUnixString(),
                },
            }) }}
        {% endfor %}
    </dna-masonry>
{% endif %}
