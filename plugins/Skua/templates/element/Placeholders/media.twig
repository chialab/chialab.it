{% if object.get('media_url')|default() %}
    {% if object.type == 'videos' %}
        <video src="{{ object.media_url }}" controls/>
    {% elseif object.type == 'images' %}
        {{ Thumb.image(object, 'default', {
            class: 'clickable',
            'data-original-url': object.media_url,
        })|raw }}
    {% endif %}
{% endif %}
