{
  "name": "chialab/chialab-it",
  "description": "Chialab.it plugin for CakePHP",
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "cakephp/cakephp": "^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.14|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Chialab\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chialab\\Test\\": "tests/",
      "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
    }
  }
}
